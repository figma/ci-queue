{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2020",
      "lib": ["es2020"],
      "noEmit": true,
      "strict": true,
      "esModuleInterop": true,
      "preserveConstEnums": true,
      "resolveJsonModule": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "types": ["node", "jasmine"],
      "typeRoots": ["./node_modules/@types", "./src/typings"]
    },

  }
  